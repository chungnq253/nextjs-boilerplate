{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@shared/*": [
        "shared/*"
      ],
      "@templates/*": [
        "templates/*"
      ],
      "@layouts/*": [
